{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
    "email": {
        "type": "string",
        "format": "email",
        "minLength": 5,
        "maxLength": 100
    },
    "password": {
        "type": "string",
        "minLength": 5,
        "maxLength": 50
    }
    },
    "required": ["email", "password"],
    "additionalProperties": false
}
  